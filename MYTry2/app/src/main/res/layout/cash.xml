<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
  >


<LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_marginTop="3dp"
        android:background="#F8F8F8"
        android:orientation="vertical">

        <TextView
            android:id="@+id/custom_top_title_bar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:fontFamily="@font/handon600g"

            android:text="마일리지"
            android:textSize="18sp"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/mileage_y"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#EEE7C087"
                android:gravity="center"
                android:text="마일리지"
                android:textAlignment="center"
                android:textColorHighlight="#00201D1D"
                android:textSize="24sp" />

            <TextView
                android:id="@+id/mileage"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="2.5dp"
                android:layout_weight="1"
                android:background="#EEE6D9"
                android:gravity="center"
                android:text="0원"
                android:textSize="18sp" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="2.5dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/cash_y"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#EEE7C087"
                android:gravity="center"
                android:text="상금"
                android:textAlignment="center"
                android:textSize="24sp" />

            <TextView
                android:id="@+id/mycash"
                android:layout_width="0dp"
                android:layout_height="87dp"
                android:layout_marginLeft="2.5dp"
                android:layout_weight="1"
                android:background="#EEE6D9"
                android:gravity="center"
                android:text="0원"
                android:textSize="18sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView5_y"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_weight="0"
                android:text="이용 내역"
                android:textSize="15sp" />

            <ListView
                android:id="@+id/cash_list"
                android:layout_width="match_parent"
                android:layout_height="485dp"

                >

            </ListView>
        </LinearLayout>

    </LinearLayout>
    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginBottom="230dp"
        android:layout_marginRight="15sp"
        app:srcCompat="@android:drawable/ic_menu_search"
        android:visibility="invisible"
        app:backgroundTint="#E99413"
        app:elevation="6dp"
        app:pressedTranslationZ="12dp" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginBottom="160dp"
        android:layout_marginRight="15sp"
        app:srcCompat="@android:drawable/ic_menu_search"
        android:visibility="invisible"
        app:backgroundTint="#E99413"
        app:elevation="6dp"
        app:pressedTranslationZ="12dp" />



    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginBottom="90dp"
        android:layout_marginRight="15sp"
        app:srcCompat="@android:drawable/ic_menu_search"
        android:visibility="invisible"
        app:backgroundTint="#E99413"
        app:elevation="6dp"
        app:pressedTranslationZ="12dp" />


    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_margin="15sp"
        app:backgroundTint="#E99413"
        app:srcCompat="@drawable/cash_coin" />



</FrameLayout>